#%RAML 1.0 DataType

type: object
properties:
  msg:
    type: string
    required: true
  code:
    type: integer
    required: true
  success:
    type: boolean
    required: true
  error:
    type: object
    required: true
    properties:
      type:
        type: string
        required: true
      title:
        type: string
        required: true
      details:
        type: string
        required: true