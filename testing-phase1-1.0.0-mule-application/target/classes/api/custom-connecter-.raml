#%RAML 1.0
title: custom-connecter 

description: This API is for generate the customConnector
protocols: [HTTPS,HTTP]
baseUri: https://jira-uat.spotify.net
mediaType: 
- application/json

/rest:
  /api:
    /{id}:
      /issue:
        post:
          headers:
            username:
              type: string
              required: true
              minLength: 5
              maxLength: 20
              example: "jirauat"
            password:
              type: string
              required: true
              minLength: 5
              maxLength: 20
              example: "spotify123"
          body:
            application/json:
              type: object
              properties:
                fields:
                  required: true
                  type: object
                  properties:
                    project:
                      required: true
                      type: object
                      properties:
                        key: 
                          required: true
                          type: string
                          example: "ITO"
                    summary:
                      required: true
                      type: string
                      example: "Demo Issue in JIRA 1233"
                    description:
                      required: true
                      type: string
                      example: "Creating of an issue using project keys and issue type names using the REST API"
                    issuetype: 
                      required: true
                      type: object
                      properties:
                        name:
                          required: true
                          type: string
                          example: "Task"
                    customfield_23515:
                      required: true
                      type: string
                      example: "Brad"
                    customfield_18127:
                      required: true
                      type: string
                      example: "costcentre123"
                    customfield_14511:
                      required: true
                      type: string
                      example: "test123@gmail.com"
                    customfield_14512:
                      required: true
                      type: string
                      example: "+91 9876543210"
                    customfield_19402:
                      required: true
                      type: string
                      example: "English"
                    customfield_14204:
                      required: true
                      type: string
                      example: "Home"
                    customfield_18611:
                      required: true
                      type: string
                      example: "2022-01-01"
                    customfield_23516:
                      required: true
                      type: string
                      example: "+91 1234567890"
                    customfield_24401:
                      required: true
                      type: string
                      example: "notes abc"
                    customfield_18610:
                      required: true
                      type: string
                      example: "ITEquipmentJira"
                    customfield_18612:
                      required: true
                      type: string
                      example: "India"
                    customfield_18613:
                      required: true
                      type: string
                      example: "Delhi"
                    customfield_23513:
                      required: true
                      type: string
                      example: "Brad Pitt"
                    customfield_18719:
                      required: true
                      type: string
                      example: "INL123"
                    customfield_23700:
                      required: true
                      type: string
                      example: "Mumbai, India"
                    customfield_23518:
                      required: true
                      type: string
                      example: "https://jira-uat.spotify.net/rest/api/2/issue/2077009"
                    customfield_18608: 
                      required: true
                      type: object
                      properties:
                        id:
                          required: true
                          type: string
                          example: "20332"
              example: !include /example/custom-example.json
          responses:
              201:
                body:
                  application/json:
                    type: object
                    properties:
                      id:
                        required: true
                        type: string
                        example: "2126917"
                      key:
                        required: true
                        type: string
                        example: "ITO-38309"
                      self:
                        required: true
                        type: string
                        example: "https://jira-uat.spotify.net/rest/api/2/issue/2126917"
                    example:
                      {
                          "id": "2126917",
                          "key": "ITO-38309",
                          "self": "https://jira-uat.spotify.net/rest/api/2/issue/2126917"
                      }
                    