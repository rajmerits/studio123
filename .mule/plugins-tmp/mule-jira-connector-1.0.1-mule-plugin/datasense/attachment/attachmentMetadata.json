{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "Attachment Schema",
  "required": [
    "id",
    "self",
    "filename",
    "author",
    "created",
    "size",
    "mimeType",
    "properties",
    "content",
    "thumbnail"
  ],
  "properties": {
    "id": {
      "$id": "#/properties/id",
      "type": "integer",
      "title": "The Id Schema",
      "default": 0,
      "examples": [
        25
      ]
    },
    "self": {
      "$id": "#/properties/self",
      "type": "string",
      "title": "The Self Schema",
      "default": "",
      "examples": [
        "<string>"
      ],
      "pattern": "^(.*)$"
    },
    "filename": {
      "$id": "#/properties/filename",
      "type": "string",
      "title": "The Filename Schema",
      "default": "",
      "examples": [
        "<string>"
      ],
      "pattern": "^(.*)$"
    },
    "author": {
      "$id": "#/properties/author",
      "type": "object",
      "title": "The Author Schema",
      "required": [
        "self",
        "key",
        "accountId",
        "accountType",
        "name",
        "emailAddress",
        "avatarUrls",
        "displayName",
        "active",
        "timeZone",
        "locale",
        "groups",
        "applicationRoles",
        "expand"
      ],
      "properties": {
        "self": {
          "$id": "#/properties/author/properties/self",
          "type": "string",
          "title": "The Self Schema",
          "default": "",
          "examples": [
            "<string>"
          ],
          "pattern": "^(.*)$"
        },
        "key": {
          "$id": "#/properties/author/properties/key",
          "type": "string",
          "title": "The Key Schema",
          "default": "",
          "examples": [
            "<string>"
          ],
          "pattern": "^(.*)$"
        },
        "accountId": {
          "$id": "#/properties/author/properties/accountId",
          "type": "string",
          "title": "The Accountid Schema",
          "default": "",
          "examples": [
            "<string>"
          ],
          "pattern": "^(.*)$"
        },
        "accountType": {
          "$id": "#/properties/author/properties/accountType",
          "type": "string",
          "title": "The Accounttype Schema",
          "default": "",
          "examples": [
            "atlassian"
          ],
          "pattern": "^(.*)$"
        },
        "name": {
          "$id": "#/properties/author/properties/name",
          "type": "string",
          "title": "The Name Schema",
          "default": "",
          "examples": [
            "<string>"
          ],
          "pattern": "^(.*)$"
        },
        "emailAddress": {
          "$id": "#/properties/author/properties/emailAddress",
          "type": "string",
          "title": "The Emailaddress Schema",
          "default": "",
          "examples": [
            "<string>"
          ],
          "pattern": "^(.*)$"
        },
        "avatarUrls": {
          "$id": "#/properties/author/properties/avatarUrls",
          "type": "object",
          "title": "The Avatarurls Schema",
          "required": [
            "16x16",
            "24x24",
            "32x32",
            "48x48"
          ],
          "properties": {
            "16x16": {
              "$id": "#/properties/author/properties/avatarUrls/properties/16x16",
              "type": "string",
              "title": "The 16x16 Schema",
              "default": "",
              "examples": [
                "<string>"
              ],
              "pattern": "^(.*)$"
            },
            "24x24": {
              "$id": "#/properties/author/properties/avatarUrls/properties/24x24",
              "type": "string",
              "title": "The 24x24 Schema",
              "default": "",
              "examples": [
                "<string>"
              ],
              "pattern": "^(.*)$"
            },
            "32x32": {
              "$id": "#/properties/author/properties/avatarUrls/properties/32x32",
              "type": "string",
              "title": "The 32x32 Schema",
              "default": "",
              "examples": [
                "<string>"
              ],
              "pattern": "^(.*)$"
            },
            "48x48": {
              "$id": "#/properties/author/properties/avatarUrls/properties/48x48",
              "type": "string",
              "title": "The 48x48 Schema",
              "default": "",
              "examples": [
                "<string>"
              ],
              "pattern": "^(.*)$"
            }
          }
        },
        "displayName": {
          "$id": "#/properties/author/properties/displayName",
          "type": "string",
          "title": "The Displayname Schema",
          "default": "",
          "examples": [
            "<string>"
          ],
          "pattern": "^(.*)$"
        },
        "active": {
          "$id": "#/properties/author/properties/active",
          "type": "boolean",
          "title": "The Active Schema",
          "default": false,
          "examples": [
            true
          ]
        },
        "timeZone": {
          "$id": "#/properties/author/properties/timeZone",
          "type": "string",
          "title": "The Timezone Schema",
          "default": "",
          "examples": [
            "<string>"
          ],
          "pattern": "^(.*)$"
        },
        "locale": {
          "$id": "#/properties/author/properties/locale",
          "type": "string",
          "title": "The Locale Schema",
          "default": "",
          "examples": [
            "<string>"
          ],
          "pattern": "^(.*)$"
        },
        "groups": {
          "$id": "#/properties/author/properties/groups",
          "type": "object",
          "title": "The Groups Schema",
          "required": [
            "size",
            "items",
            "pagingCallback",
            "callback",
            "max-results"
          ],
          "properties": {
            "size": {
              "$id": "#/properties/author/properties/groups/properties/size",
              "type": "integer",
              "title": "The Size Schema",
              "default": 0,
              "examples": [
                2154
              ]
            },
            "items": {
              "$id": "#/properties/author/properties/groups/properties/items",
              "type": "array",
              "title": "The Items Schema",
              "items": {
                "$id": "#/properties/author/properties/groups/properties/items/items",
                "type": "object",
                "title": "The Items Schema"
              }
            },
            "pagingCallback": {
              "$id": "#/properties/author/properties/groups/properties/pagingCallback",
              "type": "object",
              "title": "The Pagingcallback Schema"
            },
            "callback": {
              "$id": "#/properties/author/properties/groups/properties/callback",
              "type": "object",
              "title": "The Callback Schema"
            },
            "max-results": {
              "$id": "#/properties/author/properties/groups/properties/max-results",
              "type": "integer",
              "title": "The Max-results Schema",
              "default": 0,
              "examples": [
                2154
              ]
            }
          }
        },
        "applicationRoles": {
          "$id": "#/properties/author/properties/applicationRoles",
          "type": "object",
          "title": "The Applicationroles Schema",
          "required": [
            "size",
            "items",
            "pagingCallback",
            "callback",
            "max-results"
          ],
          "properties": {
            "size": {
              "$id": "#/properties/author/properties/applicationRoles/properties/size",
              "type": "integer",
              "title": "The Size Schema",
              "default": 0,
              "examples": [
                2154
              ]
            },
            "items": {
              "$id": "#/properties/author/properties/applicationRoles/properties/items",
              "type": "array",
              "title": "The Items Schema",
              "items": {
                "$id": "#/properties/author/properties/applicationRoles/properties/items/items",
                "type": "object",
                "title": "The Items Schema"
              }
            },
            "pagingCallback": {
              "$id": "#/properties/author/properties/applicationRoles/properties/pagingCallback",
              "type": "object",
              "title": "The Pagingcallback Schema"
            },
            "callback": {
              "$id": "#/properties/author/properties/applicationRoles/properties/callback",
              "type": "object",
              "title": "The Callback Schema"
            },
            "max-results": {
              "$id": "#/properties/author/properties/applicationRoles/properties/max-results",
              "type": "integer",
              "title": "The Max-results Schema",
              "default": 0,
              "examples": [
                2154
              ]
            }
          }
        },
        "expand": {
          "$id": "#/properties/author/properties/expand",
          "type": "string",
          "title": "The Expand Schema",
          "default": "",
          "examples": [
            "<string>"
          ],
          "pattern": "^(.*)$"
        }
      }
    },
    "created": {
      "$id": "#/properties/created",
      "type": "string",
      "title": "The Created Schema",
      "default": "",
      "examples": [
        "<string>"
      ],
      "pattern": "^(.*)$"
    },
    "size": {
      "$id": "#/properties/size",
      "type": "integer",
      "title": "The Size Schema",
      "default": 0,
      "examples": [
        27
      ]
    },
    "mimeType": {
      "$id": "#/properties/mimeType",
      "type": "string",
      "title": "The Mimetype Schema",
      "default": "",
      "examples": [
        "<string>"
      ],
      "pattern": "^(.*)$"
    },
    "properties": {
      "$id": "#/properties/properties",
      "type": "object",
      "title": "The Properties Schema"
    },
    "content": {
      "$id": "#/properties/content",
      "type": "string",
      "title": "The Content Schema",
      "default": "",
      "examples": [
        "<string>"
      ],
      "pattern": "^(.*)$"
    },
    "thumbnail": {
      "$id": "#/properties/thumbnail",
      "type": "string",
      "title": "The Thumbnail Schema",
      "default": "",
      "examples": [
        "<string>"
      ],
      "pattern": "^(.*)$"
    }
  }
}