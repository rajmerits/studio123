{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "array",
  "title": "The Root Schema",
  "items": {
    "$id": "#/items",
    "type": "object",
    "title": "The Items Schema",
    "required": [
      "self",
      "id",
      "description",
      "iconUrl",
      "name",
      "subtask",
      "avatarId"
    ],
    "properties": {
      "self": {
        "$id": "#/items/properties/self",
        "type": "string",
        "title": "The Self Schema",
        "default": "",
        "examples": [
          "http://your-domain.atlassian.net/rest/api/3/issueType/3"
        ],
        "pattern": "^(.*)$"
      },
      "id": {
        "$id": "#/items/properties/id",
        "type": "string",
        "title": "The Id Schema",
        "default": "",
        "examples": [
          "3"
        ],
        "pattern": "^(.*)$"
      },
      "description": {
        "$id": "#/items/properties/description",
        "type": "string",
        "title": "The Description Schema",
        "default": "",
        "examples": [
          "A task that needs to be done."
        ],
        "pattern": "^(.*)$"
      },
      "iconUrl": {
        "$id": "#/items/properties/iconUrl",
        "type": "string",
        "title": "The Iconurl Schema",
        "default": "",
        "examples": [
          "http://your-domain.atlassian.net//secure/viewavatar?size=xsmall&avatarId=10299&avatarType=issuetype\","
        ],
        "pattern": "^(.*)$"
      },
      "name": {
        "$id": "#/items/properties/name",
        "type": "string",
        "title": "The Name Schema",
        "default": "",
        "examples": [
          "Task"
        ],
        "pattern": "^(.*)$"
      },
      "subtask": {
        "$id": "#/items/properties/subtask",
        "type": "boolean",
        "title": "The Subtask Schema",
        "default": false,
        "examples": [
          false
        ]
      },
      "avatarId": {
        "$id": "#/items/properties/avatarId",
        "type": "integer",
        "title": "The Avatarid Schema",
        "default": 0,
        "examples": [
          1
        ]
      }
    }
  }
}
