{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "array",
  "title": "The Root Schema",
  "items": {
    "$id": "#/items",
    "type": "object",
    "title": "The Items Schema",
    "required": [
      "self",
      "id",
      "description",
      "name",
      "archived",
      "released",
      "releaseDate",
      "overdue",
      "userReleaseDate",
      "projectId"
    ],
    "properties": {
      "self": {
        "$id": "#/items/properties/self",
        "type": "string",
        "title": "The Self Schema",
        "default": "",
        "examples": [
          "https://your-domain.atlassian.net/rest/api/3/version/10000"
        ],
        "pattern": "^(.*)$"
      },
      "id": {
        "$id": "#/items/properties/id",
        "type": "string",
        "title": "The Id Schema",
        "default": "",
        "examples": [
          "10000"
        ],
        "pattern": "^(.*)$"
      },
      "description": {
        "$id": "#/items/properties/description",
        "type": "string",
        "title": "The Description Schema",
        "default": "",
        "examples": [
          "An excellent version"
        ],
        "pattern": "^(.*)$"
      },
      "name": {
        "$id": "#/items/properties/name",
        "type": "string",
        "title": "The Name Schema",
        "default": "",
        "examples": [
          "New Version 1"
        ],
        "pattern": "^(.*)$"
      },
      "archived": {
        "$id": "#/items/properties/archived",
        "type": "boolean",
        "title": "The Archived Schema",
        "default": false,
        "examples": [
          false
        ]
      },
      "released": {
        "$id": "#/items/properties/released",
        "type": "boolean",
        "title": "The Released Schema",
        "default": false,
        "examples": [
          true
        ]
      },
      "releaseDate": {
        "$id": "#/items/properties/releaseDate",
        "type": "string",
        "title": "The Releasedate Schema",
        "default": "",
        "examples": [
          "2010-07-06"
        ],
        "pattern": "^(.*)$"
      },
      "overdue": {
        "$id": "#/items/properties/overdue",
        "type": "boolean",
        "title": "The Overdue Schema",
        "default": false,
        "examples": [
          true
        ]
      },
      "userReleaseDate": {
        "$id": "#/items/properties/userReleaseDate",
        "type": "string",
        "title": "The Userreleasedate Schema",
        "default": "",
        "examples": [
          "6/Jul/2010"
        ],
        "pattern": "^(.*)$"
      },
      "projectId": {
        "$id": "#/items/properties/projectId",
        "type": "integer",
        "title": "The Projectid Schema",
        "default": 0,
        "examples": [
          10000
        ]
      }
    }
  }
}