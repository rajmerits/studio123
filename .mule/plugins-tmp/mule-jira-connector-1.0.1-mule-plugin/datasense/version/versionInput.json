{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "archived",
    "releaseDate",
    "startDate",
    "name",
    "description",
    "expand",
    "moveUnfixedIssuesTo",
    "projectId",
    "released"
  ],
  "properties": {
    "archived": {
      "$id": "#/properties/archived",
      "type": "boolean",
      "title": "The Archived Schema",
      "default": false,
      "examples": [
        false
      ]
    },
    "releaseDate": {
      "$id": "#/properties/releaseDate",
      "type": "string",
      "title": "The Releasedate Schema",
      "default": "",
      "examples": [
        "2010-07-06T00:00:00.000Z"
      ],
      "pattern": "^(.*)$"
    },
    "startDate": {
      "$id": "#/properties/startDate",
      "type": "string",
      "title": "The Startdate Schema",
      "default": "",
      "examples": [
        "2010-07-06T00:00:00.000Z"
      ],
      "pattern": "^(.*)$"
    },
    "name": {
      "$id": "#/properties/name",
      "type": "string",
      "title": "The Name Schema",
      "default": "",
      "examples": [
        "New Version 1"
      ],
      "pattern": "^(.*)$"
    },
    "description": {
      "$id": "#/properties/description",
      "type": "string",
      "title": "The Description Schema",
      "default": "",
      "examples": [
        "An excellent version"
      ],
      "pattern": "^(.*)$"
    },
    "expand": {
      "$id": "#/properties/expand",
      "type": "string",
      "title": "The Expand Schema",
      "default": "",
      "examples": [
        "operations"
      ],
      "pattern": "^(.*)$"
    },
    "moveUnfixedIssuesTo": {
      "$id": "#/properties/moveUnfixedIssuesTo",
      "type": "string",
      "title": "The Moveunfixedissuesto Schema",
      "default": "",
      "examples": [
        "moveUnfixedIssuesTo"
      ],
      "pattern": "^(.*)$"
    },
    "projectId": {
      "$id": "#/properties/projectId",
      "type": "integer",
      "title": "The Projectid Schema",
      "default": 0,
      "examples": [
        10000
      ]
    },
    "released": {
      "$id": "#/properties/released",
      "type": "boolean",
      "title": "The Released Schema",
      "default": false,
      "examples": [
        true
      ]
    }
  }
}
