{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "array",
  "title": "The Root Schema",
  "items": {
    "$id": "#/items",
    "type": "object",
    "title": "The Items Schema",
    "required": [
      "self",
      "id",
      "name",
      "description"
    ],
    "properties": {
      "self": {
        "$id": "#/items/properties/self",
        "type": "string",
        "title": "The Self Schema",
        "default": "",
        "examples": [
          "https://your-domain.atlassian.net/rest/api/3/projectCategory/10000"
        ],
        "pattern": "^(.*)$"
      },
      "id": {
        "$id": "#/items/properties/id",
        "type": "string",
        "title": "The Id Schema",
        "default": "",
        "examples": [
          "10000"
        ],
        "pattern": "^(.*)$"
      },
      "name": {
        "$id": "#/items/properties/name",
        "type": "string",
        "title": "The Name Schema",
        "default": "",
        "examples": [
          "FIRST"
        ],
        "pattern": "^(.*)$"
      },
      "description": {
        "$id": "#/items/properties/description",
        "type": "string",
        "title": "The Description Schema",
        "default": "",
        "examples": [
          "First Project Category"
        ],
        "pattern": "^(.*)$"
      }
    }
  }
}
