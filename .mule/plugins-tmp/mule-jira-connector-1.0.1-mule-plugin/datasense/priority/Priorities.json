{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "array",
  "title": "The Root Schema",
  "items": {
    "$id": "#/items",
    "type": "object",
    "title": "The Items Schema",
    "required": [
      "self",
      "statusColor",
      "description",
      "iconUrl",
      "name",
      "id"
    ],
    "properties": {
      "self": {
        "$id": "#/items/properties/self",
        "type": "string",
        "title": "The Self Schema",
        "default": "",
        "examples": [
          "https://your-domain.atlassian.net/rest/api/3/priority/3"
        ],
        "pattern": "^(.*)$"
      },
      "statusColor": {
        "$id": "#/items/properties/statusColor",
        "type": "string",
        "title": "The Statuscolor Schema",
        "default": "",
        "examples": [
          "#009900"
        ],
        "pattern": "^(.*)$"
      },
      "description": {
        "$id": "#/items/properties/description",
        "type": "string",
        "title": "The Description Schema",
        "default": "",
        "examples": [
          "Major loss of function."
        ],
        "pattern": "^(.*)$"
      },
      "iconUrl": {
        "$id": "#/items/properties/iconUrl",
        "type": "string",
        "title": "The Iconurl Schema",
        "default": "",
        "examples": [
          "https://your-domain.atlassian.net/images/icons/priorities/major.png"
        ],
        "pattern": "^(.*)$"
      },
      "name": {
        "$id": "#/items/properties/name",
        "type": "string",
        "title": "The Name Schema",
        "default": "",
        "examples": [
          "Major"
        ],
        "pattern": "^(.*)$"
      },
      "id": {
        "$id": "#/items/properties/id",
        "type": "string",
        "title": "The Id Schema",
        "default": "",
        "examples": [
          "1"
        ],
        "pattern": "^(.*)$"
      }
    }
  }
}
