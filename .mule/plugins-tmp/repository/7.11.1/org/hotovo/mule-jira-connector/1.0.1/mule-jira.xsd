  <xs:element xmlns="http://www.mulesoft.org/schema/mule/jira" xmlns:mule="http://www.mulesoft.org/schema/mule/core" type="AddUserToGroupType" substitutionGroup="mule:abstract-operator" name="add-user-to-group">
    <xs:annotation>
      <xs:documentation>Adds a user to a group.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:complexType name="AddUserToGroupType">
    <xs:complexContent>
      <xs:extension xmlns:mule="http://www.mulesoft.org/schema/mule/core" base="mule:abstractOperatorType">
