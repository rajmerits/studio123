          <xs:attribute type="mule:substitutableInt" use="optional" default="-1" name="responseBufferSize"></xs:attribute>
          <xs:attribute type="mule:expressionString" use="optional" default="https://api.atlassian.com/ex/jira" name="baseUri"></xs:attribute>
          <xs:attribute type="xs:string" use="optional" default="api.atlassian.com" name="audience"></xs:attribute>
